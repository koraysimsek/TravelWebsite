
@{
    Layout = null;
}

<!DOCTYPE html>
<html lang="en">

@Html.Action("PartialHead", "AdminLayout")
<body>
    <div class="container-scroller">
        <!-- partial:partials/_navbar.html -->
        @Html.Action("PartialNavbar", "AdminLayout")
        <!-- partial -->
        <div class="container-fluid page-body-wrapper">
            <!-- partial:partials/_sidebar.html -->
            @Html.Action("PartialMenu", "AdminLayout")
            <!-- partial -->
            <div class="main-panel">
                <div class="content-wrapper">
                    @RenderBody()
                </div>
                <!-- main-panel ends -->
            </div>
            <!-- page-body-wrapper ends -->
        </div>
    </div>
    @Html.Action("PartialScript", "AdminLayout")
</body>

</html>

