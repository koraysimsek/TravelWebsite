@model List<AcunMedyaTravelProject.Entities.Sponsor>
<div class="container">
    <div class="row row-cols-lg-5 row-cols-md-3 row-cols-2 flex-center">

        @foreach (var item in Model)
        {
            <div class="col">
                <div class="card shadow-hover mb-4" style="border-radius:10px;">
                    <div class="card-body text-center"> <img class="img-fluid" src="@item.ImageURL" alt="" /></div>
                </div>
            </div>
        }

        </div>
</div>
