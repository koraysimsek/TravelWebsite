
@{
    ViewBag.Title = "CreateTestimonial";
    Layout = "~/Views/AdminLayout/Index.cshtml";
}

<h2>Referans Oluştur</h2>

<form method="post">

    <label>Referans Adı</label>
    <input type="text" class="form-control" name="TestimonialName" style="width:200px" />
    <label>Açıklama</label>
    <input type="text" class="form-control" name="Description" style="width:200px" />
    <label>Fotoğraf</label>
    <input type="text" class="form-control" name="ImageURL" style="width:200px" />
    <label>Ünvan</label>
    <input type="text" class="form-control" name="Title" style="width:200px" />
    <button class="btn btn-success">Referans Ekle</button>

</form>